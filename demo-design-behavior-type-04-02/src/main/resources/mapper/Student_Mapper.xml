<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.design.intermediary.demo.dao.IStudentDao">
    <select id="queryStudentById" resultType="com.design.intermediary.demo.module.Student">
        select * from student where id = #{id}
    </select>
    <select id="queryStudentByAge" resultType="com.design.intermediary.demo.module.Student">
        select * from student where age = #{age}
    </select>
</mapper>